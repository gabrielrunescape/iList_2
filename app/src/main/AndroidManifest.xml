<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ilist.gabrielrunescape.com.br">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Djezzy">
        <activity
            android:name=".view.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/Djezzy.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.ItemActivity"
            android:parentActivityName=".view.HomeActivity"></activity>
        <activity android:name=".view.ReportActivity"></activity>
    </application>

</manifest>